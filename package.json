{
  "name": "generator-node-utils",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "npm": ">= 4.0.0",
    "node": "12.15.0"
  },
  "files": [
    "generators"
  ],
  "scripts": {
    "test": "jest --silent",
    "eslint-check": "eslint --print-config .eslintrc.js",
    "lint": "eslint --ignore-pattern 'generators/app/templates/' \"**/*.js\" ",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.js$ | xargs eslint",
    "lint-fix": "eslint --ignore-pattern 'generators/app/templates/' \"**/*.js\" --fix",
    "outdated": "npm outdated --depth 0",
    "pretest": "npm run lint",
    "prestart": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://url"
  },
  "author": "Wolox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wolox/express-js-bootstrap/issues",
    "email": "tls@wolox.com.ar"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "homepage": "",
  "dependencies": {
    "cfonts": "^2.5.2",
    "chalk": "^2.1.0",
    "eslint-config-wolox": "^3.0.2",
    "eslint-config-wolox-node": "^2.2.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.0.9",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.spec.js?(x)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/generators/templates"
    ],
    "clearMocks": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
